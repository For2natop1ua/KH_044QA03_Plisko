{
	"title": "GetSingleUserResource",
	"type": "object",
	"required": [
		"data",
		"support"
	],
	"properties": {
		"data": {
			"$id": "#root/data",
			"title": "Data",
			"type": "object",
			"required": [
				"id",
				"name",
				"year",
				"color",
				"pantone_value"
			],
			"properties": {
				"id": {
					"$id": "#root/data/id",
					"title": "Id",
					"type": "integer",
					"default": 0
				},
				"name": {
					"$id": "#root/data/name",
					"title": "Name",
					"type": "string",
					"default": "",
					"pattern": "^.*$"
				},
				"year": {
					"$id": "#root/data/year",
					"title": "Year",
					"type": "integer",
					"default": 0
				},
				"color": {
					"$id": "#root/data/color",
					"title": "Color",
					"type": "string",
					"default": "",
					"pattern": "^.*$"
				},
				"pantone_value": {
					"$id": "#root/data/pantone_value",
					"title": "Pantone_value",
					"type": "string",
					"default": "",
					"pattern": "^.*$"
				}
			}
		}
,
		"support": {
			"$id": "#root/support",
			"title": "Support",
			"type": "object",
			"required": [
				"url",
				"text"
			],
			"properties": {
				"url": {
					"$id": "#root/support/url",
					"title": "Url",
					"type": "string",
					"default": "",
					"pattern": "^.*$"
				},
				"text": {
					"$id": "#root/support/text",
					"title": "Text",
					"type": "string",
					"default": "",
					"pattern": "^.*$"
				}
			}
		}

	}
}
