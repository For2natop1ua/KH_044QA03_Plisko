{
  "title": "getSingleUser",
  "type": "object",
  "required": [
    "data",
    "support"
  ],
  "properties": {
    "data": {
      "$id": "#root/data",
      "title": "Data",
      "type": "object",
      "required": [
        "id",
        "email",
        "first_name",
        "last_name",
        "avatar"
      ],
      "properties": {
        "id": {
          "$id": "#root/data/id",
          "title": "Id",
          "type": "integer",
          "default": 0
        },
        "email": {
          "$id": "#root/data/email",
          "title": "Email",
          "type": "string",
          "default": "",
          "pattern": "^.*$"
        },
        "first_name": {
          "$id": "#root/data/first_name",
          "title": "First_name",
          "type": "string",
          "default": "",
          "pattern": "^.*$"
        },
        "last_name": {
          "$id": "#root/data/last_name",
          "title": "Last_name",
          "type": "string",
          "default": "",
          "pattern": "^.*$"
        },
        "avatar": {
          "$id": "#root/data/avatar",
          "title": "Avatar",
          "type": "string",
          "default": "",
          "pattern": "^.*$"
        }
      }
    }
  ,
    "support": {
      "$id": "#root/support",
      "title": "Support",
      "type": "object",
      "required": [
        "url",
        "text"
      ],
      "properties": {
        "url": {
          "$id": "#root/support/url",
          "title": "Url",
          "type": "string",
          "default": "",
          "pattern": "^.*$"
        },
        "text": {
          "$id": "#root/support/text",
          "title": "Text",
          "type": "string",
          "default": "",
          "pattern": "^.*$"
        }
      }
    }

  }
}
